{
  "name": "gst-components",
  "version": "0.0.1-beta.15",
  "description": "Set of Angular components",
  "main": "build/gst-components.js",
  "types": "build/gst-components.d.ts",
  "scripts": {
    "build:component": "gulp build",
    "build:demo": "npm run clean:dist && npm run webpack -- --config config/webpack.dev.js --progress --profile",
    "ci:nobuild": "npm test && npm run build:demo && npm run e2e",
    "ci:travis": "npm run test && npm run build:demo && npm run e2e:travis",
    "ci": "npm run ci:testall",
    "clean:dll": "npm run rimraf -- dll",
    "clean:aot": "npm run rimraf -- compiled",
    "clean:dist": "npm run rimraf -- dist build",
    "clean:install": "npm set progress=false && npm install",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc coverage dist compiled dll",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "e2e:concurency": "npm run build:demo && concurrently -s first --name \"appserver,protractor\" -p \"{name}--{pid}\" -k \"npm run server:test:ci\" \"sleep 10 && npm run protractor --verbose\"",
    "e2e:live": "npm-run-all -p -r server:test:ci protractor:live",
    "e2e:travis": "npm-run-all -p -r server:test:ci protractor:delay",
    "e2e": "npm-run-all -p -r server:test:ci protractor",
    "lint": "npm run tslint \"components/**/*.ts\"",
    "node": "node",
    "postinstall": "npm run webdriver:update",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "npm run webdriver-manager start",
    "webdriver:update": "webdriver-manager update",
    "preclean:install": "npm run clean",
    "protractor": "protractor",
    "protractor:delay": "sleep 6 && npm run protractor",
    "protractor:live": "protractor --elementExplorer",
    "rimraf": "rimraf",
    "server:dev:hmr": "npm run server:dev -- --inline --hot",
    "server:dev": "npm run webpack-dev-server -- --config config/webpack.dev.js --progress --profile --watch --content-base src/",
    "server:test:ci": "superstatic -c superstatic.json --port 3000 dist",
    "server": "npm run server:dev",
    "test": "npm run lint && karma start",
    "tslint": "tslint",
    "typedoc": "typedoc",
    "webpack-dev-server": "node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "webpack": "node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js"
  },
  "keywords": [
    "Anguar",
    "GST",
    "Components"
  ],
  "homepage": "https://github.com/GestionSystemesTelecom/angular-components",
  "license": "MIT",
  "author": "Valérian Girard<valerian.girard@cfast-web.fr>",
  "contributors": [
    {
      "name": "Jérémy Bolzonella",
      "email": "jeremy.bolzonella@cfast-web.fr"
    }
  ],
